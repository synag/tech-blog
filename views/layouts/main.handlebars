    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" />
    <link rel="stylesheet" href="/css/style.css" />
    
    </head>
    
    <body>
    
      <nav class="navbar navbar-expand-lg navbar-light navbar-dark bg-dark">
    
        {{#if logged_in}}
        <div class="navbar-brand text-warning">Tech Blog Home</div>
        {{else}}
        <div class="navbar-brand text-warning">Tech Blog</div>
        {{/if}}
    
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav"
          aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
          <ul class="navbar-nav">
           
            <li class="nav-item">
              <a class="nav-link anchorHomepage" type="button" aria-current="page" data-toggle="button" aria-pressed="false"
                autocomplete="off">Home</a>
            </li>
            <li class="nav-item">
              <a class="nav-link anchorDashboard" data-toggle="button" aria-pressed="false" autocomplete="off">Dashboard</a>
            </li>
           
            <li class="nav-item">
              <a class="nav-link anchorLogin" data-toggle="button" aria-pressed="false" autocomplete="off">Login</a>
            </li>
            
            <li class="nav-item">
              <a class="nav-link" id="logout" data-toggle="button" aria-pressed="false" autocomplete="off">Logout</a>
            </li>
            <script src="./js/logout.js"></script>
          </ul>
        </div>
    
      </nav>
    
      <div class="container containerMarginTop">
        {{{body}}}
      </div>
      <script src="./js/index.js"></script>
    
    
      <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
