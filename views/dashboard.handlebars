{{#each users}}
<div class="card">
  <div class="card-header">
   {{title}}
  </div>
  <div class="card-body">
    <blockquote class="blockquote mb-0">
      <p>{{content}}</p>
      <footer class="blockquote-footer">Someone famous in <cite title="Source Title">Source Title</cite></footer>
    </blockquote>
  </div>
</div>
 {{/each}}

<div class="d-grid gap-2 col-6 mx-auto createButton">
  <button class="btn btn-primary createBtn" type="button">NEW POST</button>
</div>