{{#each users}}
<div class="card">
 <a id="{{id}}" data-bs-toggle="tooltip" data-bs-placement="top" title="click title to update or delete post"><div class="card-header updatePost">
  <span>Title: </span> {{title}}
  </div></a>
  <div class="card-body">
    <blockquote class="blockquote mb-0">
      <p><span>Content: </span>{{content}}</p>
      <footer class="blockquote-footer">Creation Date: <cite title="Source Title">{{createdDate}}</cite></footer>
    </blockquote>
  </div>
</div>
 {{/each}}

<div class="d-grid gap-2 col-6 mx-auto createBtn fixed-bottom">
  <button class="btn btn-primary createBtn" type="button">NEW POST</button>
</div>
