{{#each blogs}}
<div class="card">
   <a id="{{id}}"><div class="card-header commentPost">
    <span>Title: </span> {{title}}
  </div></a>
  <div class="card-body">
    <blockquote class="blockquote mb-0">
      <p><span>Content: </span>{{content}}</p>
      <footer class="blockquote-footer">Creation Date: <cite title="Source Title">{{createdDate}}</cite></footer>
    </blockquote>
  </div>
</div>
 {{/each}}


